alert tcp any any -> $HOME_NET 22 (sid: 1;)
pass tcp any any -> $HOME_NET 22 (sid: 2;)
alert tcp $HOME_NET any -> any 80 (sid: 3;)
pass tcp $HOME_NET any -> any 80 (sid: 4;)
alert tcp $HOME_NET any -> any 443 (sid: 5;)
pass tcp $HOME_NET any -> any 443 (sid: 6;)
alert icmp $HOME_NET any -> any any (sid: 7;)
pass icmp $HOME_NET any -> any any (sid: 8;)
drop ip any any <> any any (msg:"Drop All Packets"; sid:9999;)